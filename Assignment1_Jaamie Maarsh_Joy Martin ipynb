    "}\n",
    "#mapping the corresponding state to its division\n",
    "\n",
    "df[\"Division\"] = df[\"Provider State\"].map(state_to_division)\n",
    "nursing_facilities_division = df.groupby('Division')['Provider Name'].count().reset_index()\n",
    "print(\"Number of Nursing facilities by Division\")\n",
    "print(nursing_facilities_division)"
   ]
  },

    "# Part-2 of the Answer:\n",
    "\n",
    "#Percentage calculation of the bed occupancy in Middlesex\n",
    "\n",
    "Middlesex_df['Percentage occupancy in %'] = (Middlesex_df['Total Number of Occupied Beds'] / Middlesex_df['Number of All Beds']) * 100\n",
    "\n",
    "#Displaying the necessary columns \n",
    "\n",
    "print(\"Percentage occupancy of bed in Middlesex\")\n",
    "\n",
    "display (Middlesex_df[['Federal Provider Number', 'Provider Name' , 'Percentage occupancy in %']])\n"
   ]
